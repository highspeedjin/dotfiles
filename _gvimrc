scriptencoding utf-8

set showtabline=2               " タフ?を常に表示
colorscheme hybrid              " カラースキーマ
syntax on
set number                      " 行番号表示
set antialias                   " アンチエイリアス
set guifont=VL_Gothic:h10       " 半角文字の設定
set guifontwide=VL_Gothic:h10   " 全角文字の設定
set columns=150                 " 横幅
set lines=40                    " 行数
